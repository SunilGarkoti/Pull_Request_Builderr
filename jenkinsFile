pipeline { 
    agent any 
    
  tools {
  jdk 'Java_Home'
  maven 'Maven_Home'
   }
    
    
    stages {
        stage('Initialize') { 
            steps { 
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                '''
            }
        }
       
      
       try{
         
         stage ('Build') {
            steps {
              git credentialsId: 'git_username_password', url: 'https://github.com/SunilGarkoti/jenkins.git'
              withMaven {
              sh "mvn clean verify"
            } 
          
           }} }catch(e){
             build_ok = false
           }
    
    
     stage('three') {
        sh 'exit 0'
    }

    if(build_ok) {
        currentBuild.result = "SUCCESS"
    } else {
        currentBuild.result = "FAILURE"
    }
    
}





