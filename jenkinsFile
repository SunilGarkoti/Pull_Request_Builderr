
pipeline { 
    agent any 
    
    stages {
        stage('Build') { 
            steps { 
                echo "Build is running" 
            }
        }
       
      
       stage('Test'){
          
          steps {
                echo "Test is running"       
          
          script {
          try {
          sh 'do your stuff'
          build 'Maven_Project'   
          } catch (Exception e) {
             sh 'Handle the exception!'
                }
                  }
            }
        }
        stage('Deploy') {
            steps {
                echo "Deployement is on processs"
                 sh "/Users/sunilgarkoti/pipeline/make.sh"
            }
        }
    }
}
