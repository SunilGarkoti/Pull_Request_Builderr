pipeline { 
    agent any 
    
  tools {
  jdk 'Java_Home'
  maven 'Maven_Home'
   }
    
    
    stages {
        stage('Initialize') { 
            steps { 
                sh '''
                    echo "PATH = ${PATH}"
                    echo "M2_HOME = ${M2_HOME}"
                '''
            }
        }
       
      
       
         
         stage ('Build') {
            steps {
              git credentialsId: 'git_username_password', url: 'https://github.com/SunilGarkoti/jenkins.git'
              withMaven {
              sh "mvn clean verify"
            } 
          
           }} post {
             always {
            currentBuild.result = "SUCCESS"
        }
    }
    
    
     stage('three') {
        echo "Test is successful"
    }

   }
    
}
